# Backend environment template
# Copy to .env and fill in values

# Path to ONNX segmentation model inside container (mounted volume)
MODEL_PATH=/models/segformer_b0_ade20k.onnx

# CPU threads for ONNXRuntime
ORT_THREADS=1

# Firebase Storage
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
# Mounted service account key path inside container
GOOGLE_APPLICATION_CREDENTIALS=/secrets/service_account.json
# Make uploaded images public (true) or use signed URLs (false)
PUBLIC_READ=true

# Processing limits
MAX_IMAGE_SIDE=1600
MIN_MASK_RATIO=0.08

# Class mapping (adjust to your model)
WALL_CLASS=12
BUILDING_CLASS=2

# CORS origins (comma-separated). Example for web/app domains:
CORS_ORIGINS=https://your.app.domain,https://your.web.domain
